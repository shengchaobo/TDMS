<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="t16" namespace="/pages/T16" extends="struts-default">
		<!-- 添加数据 -->
	    <action name="insert" class="cn.nit.action.table1.T16Action" method="insert" />
	    <!-- 显示数据 -->
	    <action name="auditingData" class="cn.nit.action.table1.T16Action" method="auditingData" />
	     <!-- 编辑数据 -->
	    <action name="edit" class="cn.nit.action.table1.T16Action" method="edit" />
	    <!-- 删除数据 -->
	    <action name="deleteByIds" class="cn.nit.action.table1.T16Action" method="deleteCoursesByIds" />
	      <!-- 数据查询 -->
	    <action name="singleSearch" class="cn.nit.action.table1.T16Action" method="auditingConditions"/>
	    
	     <!-- 文件批量上传 -->
	    <action name="uploadFile" class="cn.nit.action.UploadAction" method="uploadFile">
	    	<!-- 上传文件对应的处理类 -->
	    	<param name="className">cn.nit.excel.imports.table1.UndergraCSBaseTeaExcel</param>
	    	<!-- 处理类对应的方法名 -->
	    	<param name="methodName">batchInsert</param>
	    </action>
	</package>
</struts>