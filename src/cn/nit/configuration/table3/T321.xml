<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="MainTrainBasicInfoTea" namespace="/pages/MainTrainBasicInfoTea" extends="struts-default">
		<!-- 大类培养基本情况 -->
	    <action name="insert" class="cn.nit.action.table3.T321_Action" method="insert" />
	    
	    	    <!-- 显示 -->
	     <action name="auditingData" class="cn.nit.action.table3.T321_Action" method="auditingData" />
	     	     <!-- 编辑数据 -->
	    <action name="edit" class="cn.nit.action.table3.T321_Action" method="edit" />
	    
	     <!-- 删除数据 -->
	    <action name="deleteCoursesByIds" class="cn.nit.action.table3.T321_Action" method="deleteCoursesByIds" />
	    
	    	     <!-- 数据查询 -->
	    <action name="singleSearch" class="cn.nit.action.table3.T321_Action" method="auditingConditions"/>
	    
	    	   	      <!-- 数据导出 -->
	    <action name="dataExport" class="cn.nit.action.table3.T321_Action" method="execute">
	    	<result name="success" type="stream">
	    		<!-- 指定下载文件的类型，下面表示无限制 -->
				<param name="contentType">application/octet-stream;charset=UTF-8</param>
				<!-- 流对象名字 -->
				<param name="inputName">inputStream</param>
				<!-- 文件名转码 -->
				<param name="contentDisposition">attachment;fileName="${excelName}"</param>
				<!-- 下载文件缓冲大小 -->
				<param name="bufferSize">4096</param>
	    	</result>
	    </action>
	    
	</package>
</struts>

