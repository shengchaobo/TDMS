<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="t551" namespace="/pages/T551" extends="struts-default">
		<!-- 添加获奖 -->
	    <action name="insert" class="cn.nit.action.table5.T551Action" method="insert" />
	    <action name="auditingData" class="cn.nit.action.table5.T551Action" method="auditingData" />
	     <!-- 编辑数据 -->
	    <action name="edit" class="cn.nit.action.table5.T551Action" method="edit" />
	     <!-- 删除数据 -->
	    <action name="deleteByIds" class="cn.nit.action.table5.T551Action" method="deleteCoursesByIds" />
	     <!-- 数据查询 -->
	    <action name="singleSearch" class="cn.nit.action.table5.T551Action" method="auditingConditions"/>
	    
	     <!-- 文件批量上传 -->
	    <action name="uploadFile" class="cn.nit.action.UploadAction" method="uploadFile">
	    	<!-- 上传文件对应的处理类 -->
	    	<param name="className">cn.nit.excel.imports.table1.T551Excel</param>
	    	<!-- 处理类对应的方法名 -->
	    	<param name="methodName">batchInsert</param>
	    </action>
	    
	     <!-- 模板下载 -->
	    <action name="downloadModel" class="cn.nit.action.DownloadModelAction" method="execute">
	    <!-- 模板路径 -->
	    	<param name="fileName">cn/nit/excel/downloads/T551.xls</param>
	    	<result name="success" type="stream">
	    		<!-- 指定下载文件的类型，下面表示无限制 -->
				<param name="contentType">application/octet-stream;charset=UTF-8</param>
				<!-- 流对象名字 -->
				<param name="inputName">inputStream</param>
				<!-- 文件名转码 -->
				<param name="contentDisposition">attachment;filename="${saveFile}"</param>
				<!-- 下载文件缓冲大小 -->
				<param name="bufferSize">4096</param>
	    	</result>
	    </action>
	    
	       <!-- 数据导出 -->
	    <action name="dataExport" class="cn.nit.action.table5.T551Action" method="execute">
	    	<result name="success" type="stream">
	    		<!-- 指定下载文件的类型，下面表示无限制 -->
				<param name="contentType">application/octet-stream;charset=UTF-8</param>
				<!-- 流对象名字 -->
				<param name="inputName">inputStream</param>
				<!-- 文件名转码 -->
				<param name="contentDisposition">attachment;filename="${excelName}"</param>
				<!-- 下载文件缓冲大小 -->
				<param name="bufferSize">4096</param>
	    	</result>
	    </action>
	</package>
</struts>