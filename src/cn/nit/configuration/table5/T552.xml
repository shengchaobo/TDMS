<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">

<struts>
	<package name="pages/T552" namespace="/pages/T552" extends="struts-default">
		<!-- 添加课程库 -->
	    <action name="insert" class="cn.nit.action.table5.T552_Action" method="insert" />
	    <!-- 加载待审核数据 -->
	    <action name="auditingData" class="cn.nit.action.table5.T552_Action" method="auditingData" />
	    <!-- 数据编辑 -->
	    <action name="edit" class="cn.nit.action.table5.T552_Action" method="edit" />
	     <!-- 删除数据 -->
	    <action name="deleteByIds" class="cn.nit.action.table5.T552_Action" method="deleteCoursesByIds" />
	   
	   
	      <!-- 文件批量上传 -->
	    <action name="uploadFile" class="cn.nit.action.UploadAction" method="uploadFile">
	    	<!-- 上传文件对应的处理类 -->
	    	<param name="className">cn.nit.excel.imports.table5.T552_Excel</param>
	    	<!-- 处理类对应的方法名 -->
	    	<param name="methodName">batchInsert</param>
	    </action> 
	    
	      <!-- 模板下载 -->
	    <action name="downloadModel" class="cn.nit.action.DownloadModelAction" method="execute">
	    <!-- 模板路径 -->
	    	<param name="fileName">cn/nit/excel/downloads/T552.xls</param>
	    	<result name="success" type="stream">
	    		<!-- 指定下载文件的类型，下面表示无限制 -->
				<param name="contentType">application/octet-stream;charset=UTF-8</param>
				<!-- 流对象名字 -->
				<param name="inputName">inputStream</param>
				<!-- 文件名转码 -->
				<param name="contentDisposition">attachment;filename="${saveFile}"</param>
				<!-- 下载文件缓冲大小 -->
				<param name="bufferSize">4096</param>
	    	</result>
	    </action>
	    
	    
	   <!-- 数据导出 -->
	    <action name="dataExport" class="cn.nit.action.table5.T552_Action" method="execute">
	    	<result name="success" type="stream">
	    		<!-- 指定下载文件的类型，下面表示无限制 -->
				<param name="contentType">application/octet-stream;charset=UTF-8</param>
				<!-- 流对象名字 -->
				<param name="inputName">inputStream</param>
				<!-- 文件名转码 -->
				<param name="contentDisposition">attachment;fileName="${excelName}.xls"</param>
				<!-- 下载文件缓冲大小 -->
				<param name="bufferSize">4096</param>
	    	</result>
	    </action>	    	    
	</package>
</struts>